- load static slick_tags
!!!
%html{lang: "{{LANGUAGE_CODE}}"}
  %head

    %meta{charset: "utf-8"}/
    %meta{content: "width=device-width, initial-scale=1.0", name: "viewport"}
    %meta{content: "", name: "description"}
    %meta{content: "", name: "author"}
    %title Slick
    - block stylesheets
      %link{href: "http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700", rel: "stylesheet"}
      %link{href: "{% static 'slick/plugins/bootstrap-select-1.6.3/css/bootstrap-select.min.css' %}", rel: "stylesheet"}
      %link{href: "{% static 'slick/css/slick.css' %}", rel: "stylesheet"}
    - block javascripts
      %script{src: "http://code.jquery.com/jquery-2.1.1.min.js"}
      %script{src: "{% static 'slick/plugins/jquery-slimscroll-1.3.0/jquery.slimscroll.min.js' %}"}
      %script{src: "{% static 'slick/plugins/bootstrap-3.3.0/js/bootstrap.min.js' %}"}
      %script{src: "//ajax.googleapis.com/ajax/libs/angularjs/1.3.8/angular.min.js"}
      %script{src: "//ajax.googleapis.com/ajax/libs/angularjs/1.3.8/angular-resource.min.js"}
      %script{src: "//ajax.googleapis.com/ajax/libs/angularjs/1.3.8/angular-sanitize.min.js"}
      %script{src: "https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.13/angular-ui-router.min.js"}
      %script{src: "{% static 'slick/js/tv4.min.js' %}"}
      %script{src: "{% static 'slick/js/ObjectPath.js' %}"}
      %script{src: "{% static 'slick/js/schema-form.min.js' %}"}
      %script{src: "{% static 'slick/js/angular-slimscroll.js' %}"}
      %script{src: "{% static 'slick/js/bootstrap-decorator.min.js' %}"}
    
    - block extrahead

  %body{:ng-app => "slick"}
    #page-container
      
      - if not is_popup
        #header.header.navbar.navbar-default.navbar-fixed-top
            .navbar-header
              - block branding
                %a.navbar-brand{}
                  - block admin_title
                    - slick_admin_title

              %button.navbar-toggle{:data-click => "sidebar-toggle", :type => "button"}
                %span.icon-bar
                %span.icon-bar
                %span.icon-bar
             
           - block object-tools
            .nav.navbar-nav.navbar-right
              %ul.list-inline
                - block object-tools-items
        %aside.aside
          #sidebar.sidebar
            %div.scroll-body{:slimscroll => "{railVisible: true, height: auto}"}
              - include "slick/partials/sidebar.html"
      
      #content.content

        - block messages
          - if messages
            
              - for message in messages
                .note{:class => "{% if message.tags %}note-{{ message.tags }}{% endif %}"}
                  {{ message }}

        %ul#breadcrumbs.list-inline.pull-right
          - block breadcrumbs
          - if title
            %li
              {{ title|capfirst }}

        %h1.content-title
          - block content_title
            - if title
              {{ title }}

        <div ui-view></div>
        - block content
            
    

    %script{src: "{% static 'slick/js/angular.js' %}"}
    - block footer
